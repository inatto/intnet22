         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         IsSelected="False">

    <TabItem.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../../../Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </TabItem.Resources>


    <TabItem.Header>
        <StackPanel>
            <Image Style="{StaticResource ImageStyle}" Name="ImgIcon" />
            <TextBlock Text="Jurídico" FontSize="12" />
